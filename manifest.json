{
  "manifest_version": 2,
  "name": "HNK Speed Dial",
  "version": "0.0.1",

  "description": "HNK Speed Dial - lightweight extension",

  "author": "pgdba",

  "browser_action": {
    "default_icon": "assets/icon.png",
    "default_popup": "src/popup.html",
    "default_title": "HNK Speed Dial"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "src/newTab.html"
  }
}
